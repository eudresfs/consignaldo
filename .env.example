# Ambiente
NODE_ENV=development
PORT=3000

# Database
DATABASE_URL="postgresql://user:password@localhost:5432/consignaldo?schema=public"

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT
JWT_SECRET=your-secret-key
JWT_EXPIRATION=1h

# Logging
LOG_LEVEL=debug

# Rate Limiting
RATE_LIMIT_WINDOW=900000  # 15 minutos em milissegundos
RATE_LIMIT_MAX=100

# APIs Externas
API_CONSIGNANTE_URL=https://api.consignante.com.br
API_BANCO_URL=https://api.banco.com.br

# MÃ©tricas
ENABLE_METRICS=true
METRICS_PORT=9090
